
<!-- Navbar auto -->


<div class="wrapper">
  <!-- Toolbar -->

  <div class="toolbar">



  </div>

  <!-- Tables#index -->

  <div class="wrapper-tables">
    <div class="container-fluid">
      <div class="row">
        <% @tables.each do |table| %>
          <div class="col-md-4 col-sm-6 col-xs-12">
            <div class="table-section">
              <div class="table-card-name"> <h3>table.name</h3></div>
              <div class="table-card">
                <div class="avatar-frame">
                  <% @nbhaut.times do %>
                    <div class="avatar"></div>
                  <% end %>
                </div>
                <div class="table-frame"></div>
                <div class="avatar-frame">
                  <% @nbbas.times do %>
                    <div class="avatar"></div>
                  <% end %>
                </div>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>

</div>
<!-- Footer -->
